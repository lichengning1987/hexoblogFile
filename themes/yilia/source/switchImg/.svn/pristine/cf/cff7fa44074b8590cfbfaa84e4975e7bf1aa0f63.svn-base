/*发展历程图*/
.ln-harmony-Course{background:#f5f1ef url("../img/pro/bottomshadow_01.png") repeat-x left bottom;padding-bottom:20px;}
.Course-title{background:url("../img/pro/harmonyrep_03.jpg") repeat-x;height:100px;line-height: 100px;text-align: center;font-size:26px;color:#c19e8b;}
.Course-year{cursor:pointer;width:50px;height:50px;font-size:16px;background:url("../img/pro/icon-year.png") no-repeat;color:#e60000;text-align:center;display:inline-block;vertical-align: middle;}
.Course-year .icon-year{margin: 5px 0 0 18px;background:url("../img/pro/icon-arrows.png") no-repeat;width:14px;height:6px;display:block;}
.Course-year .icon-arrowdown{background-position:-1px -3px;}
.Course-year .icon-arrowup{background-position:-23px -3px;}
.Course-year span{display:block;padding-top:14px;}
.Course-yearbox,.Course-lastpoint{position:relative;text-align:center;}
.yearline{background:#fff;width:2px;display:inline-block;}
.Course-yearinfo{position:relative;}
.Course-month{position:absolute;left:0px;top:10px;}
.Course-month li{width:460px;background:#fff;position:relative;}
.Course-month li h4{line-height: 56px;font-size:16px;color:#e60000;}
.Course-month li h4 em{font-size:36px;}
.Course-month li img{width:460px;}
.Course-month li .month-title{line-height: 20px;font-size:20px;padding-bottom:15px;color:#272727;}
.Course-month li.month-left{text-align:right;}
.Course-month li.month-right{position:relative;left:520px;text-align:left;}
.Course-month li.month-left .point{position:absolute;right:-35px;top:25px;background:url("../img/pro/yuanpoint.png") no-repeat;width:10px;height:10px;}
.Course-month li.month-right .point{position:absolute;left:-35px;top:25px;background:url("../img/pro/yuanpoint.png") no-repeat;width:10px;height:10px;}
.Course-month li.month-left .icon-triangle{background:url("../img/pro/leftsan.jpg") no-repeat;width:12px;height:17px;display:block;position:absolute;right:-12px;top:21px;}
.Course-month li.month-right .icon-triangle{background:url("../img/pro/rightsan.jpg") no-repeat;width:12px;height:17px;display:block;position:absolute;left:-12px;top:21px;}
.Course-month li.month-left  h4{padding-right:15px;}
.Course-month li.month-left  em{padding-right:10px;}
.Course-month li.month-right  h4{padding-left:15px;}
.Course-month li.month-right  em{padding-right:10px;}
.Course-month li.month-left  .month-title{padding-right:15px;}
.Course-month li.month-right  .month-title{padding-left:15px;}
.Course-lastpoint{margin-top:-4px;}
.Course-lastpoint .point{width:10px;height:10px;display:inline-block;vertical-align: middle;background:url("../img/pro/icon-white.png") no-repeat;}
.Course-lastpoint .line{display:inline-block;width:2px;height:10px;background-color:#fff;}


