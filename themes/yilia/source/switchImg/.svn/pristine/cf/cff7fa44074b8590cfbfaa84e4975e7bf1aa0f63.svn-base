
/*CSS reset*/
body, div, p, ul, ol, li, dl, dt, dd, fieldset, legend, form, hr, table, h1, h2, h3, h4, h5, h6, address, blockquote, dir, td, th, button, b {margin:0; padding:0;color:#333;font: 12px/1  "Microsoft Yahei", "Arial", "Verda", "tahoma", 'Helvetica', "\5b8b\4f53", sans-serif;}
ul, ol {list-style:none;}
fieldset, legend, img {border:none;}
legend {display:none;}
em {font-style:normal;}
h1, h2, h3, h4, h5, h6, b {font-weight:normal;}
table {border-collapse:collapse; border-spacing:0;}
button {cursor:pointer;}
textarea {resize:none;}
i{ font-style: normal;}
a{text-decoration: none;color: #585858;}
.clearfix:after {clear:both; content:"."; display:block; height:0; line-height:0; font-size:0;  visibility:hidden; }
.clearfix { *zoom:1; }
.icon{ display: inline-block; overflow: hidden;margin-right: 3px; background: url(../img/all.png) no-repeat 0 0; vertical-align: middle;}
.hide{display: none;}
h2{font:400 20px/normal 'Microsoft Yahei';}
.must{color: red;}
.controlWrap .borderRed,.controlWrap .borderRed:focus,.borderRed,.travelPlaces.borderRed{ border-color: #B94A48;box-shadow: 0 1px 1px rgba(0, 0, 0, 0.075) inset;}
body .row .btn,.btn{outline: none;}
.btn:focus{outline: none;}
.center{ width: 980px; margin: 0 auto;  /*background-color: #ccc;*/}
html{
    font-size: 100%;
    -ms-text-size-adjust: 100%;
    -webkit-text-size-adjust: 100%;
    width:100%;
    height:100%;
}
.w1000{width:1000px;}
.w980{width:980px;}
.w900{width:900px;}
.margin-auto{margin:0 auto;}
.margintp50{margin-top:-50px;}
.box-shadow {
    box-shadow: 0px 8px 8px #DCD5D1 inset;
}
.tpshadow{
    background:url("../img/pro/box-shadow.png") repeat-x;width:100%; position:absolute;left:0px;top:0px;
    height:12px;
}
.cultureshadow{
    background:url("../img/pro/culshadow.png") repeat-x;width:100%; position:absolute;left:0px;top:0px;
    height:13px;
}

body{
    font-family: "Microsoft Yahei", "Arial", "Verda", "tahoma";
}

/*header*/
.ln-header{}
.ln-header .nav{background:url("../img/pro/NAVS.png") repeat-x;color:#fff;position:fixed;left:0px;top:0px;width:100%;z-index:444;height:50px;box-shadow: 0 2px 50px rgba(0,0,0,.5);}
.ln-header .nav-change{

    background-color:#000;opacity:0.8;
    -webkit-transition:  background-color 0.5s ease 0.2s;
    -moz-transition:  background-color 0.5s ease 0.2s;
    -o-transition:  background-color 0.5s ease 0.2s;
    transition: background-color 0.5s ease 0.2s;
    height:50px;
}
.ln-header .firstlogo{color:#fff;font-size:26px;line-height: 46px;display:inline;float:left;margin-right:16px;}
.ln-header .navbar{}
.ln-header .navbox{position:relative;height:50px;}
.ln-header .navbar li{float:left;line-height: 48px;font-size:18px;width:90px;margin-left:50px;text-align:center;}
.ln-header .navbar li a{color:#fff;display:block;}
.ln-header .navline{width:90px;height:2px;background:#e60000;position:absolute;top:48px;left:182px;display:none;
   /* -webkit-transition:  all 0.1s linear;
    -moz-transition:  all 0.1s linear;
    -o-transition:  all 0.1s linear;
    transition: all 0.1s linear;*/}
.ln-header .navbox .logo{background:url("../img/pro/harmonylogo.png") no-repeat;width:107px;height:25px;display:block;text-indent:-99999px;float:left;margin:10px;}


.ln-header-product{background:url("../img/pro/product-big_01.jpg") no-repeat center top;width:100%;height:801px;}
.ln-header-other{background:url("../img/pro/otherimg_01_01.jpg") no-repeat center top;width:100%;height:800px;}
.ln-header-harmony{background:url("../img/pro/harmonyimg_01.jpg") no-repeat center top;height:800px;}
@media (max-height:1080px){
    .ln-header-product{background:url("../img/pro/product-big_01.jpg") no-repeat center top;width:100%;height:801px;}
    .ln-header-other{background:url("../img/pro/otherimg_01_01.jpg") no-repeat center top;width:100%;height:800px;}
    .ln-header-harmony{background:url("../img/pro/harmonyimg_01.jpg") no-repeat center top;width:100%;height:800px;}
}
@media (max-height:900px){
    .ln-header-product,.ln-header-other{background-position:50% -100px;height:700px;}
    .ln-header-harmony{background-position:center top;height:800px;}
}
@media (max-height:768px){
    .ln-header-product{background:url("../img/pro/product-small_01.jpg") no-repeat center top;width:100%;height:520px;background-position:50% -50px;}
    .ln-header-other{background:url("../img/pro/otherimg_01small_01.jpg") no-repeat center top;width:100%;height:519px;background-position:50% -50px;}
/*    .ln-header-harmony{background:url("../img/pro/harmonyimg_01small.jpg") no-repeat center top;height:569px;}*/
    .ln-header .firstlogo{margin-left:100px;}
    .ln-header .navbox  .logo{margin:10px 20px 10px 100px;}
    .ln-header .navbar li{margin-left:12px;}
    .ln-header .navline{left:240px;}
}

/*foot*/
.ln-foot{
    background:#fff;height:188px;
}
.ln-footer{ position:relative;color:#caad9d;}
.ln-foot p{ font-family: "Microsoft Yahei", "Arial", "Verda", "tahoma";position:absolute;left:0px;top:130px;}
.ln-footlogo{background:url("../img/pro/harmonyLog.png") no-repeat;width:15px;height:17px;display:inline-block;zoom:1;vertical-align: middle;}
.ln-footback{position:absolute;right:50px;top:130px;color:#caad9d;}
.ln-footop{position:absolute;right:15px;top:130px;color:#caad9d;}

/*contents*/
.product-box{background-color:#f5f1ef;position:relative;padding-bottom:20px;}
.product-title{text-align: center;border-bottom:2px solid #fff;}
.product-title .icon{display:inline-block;vertical-align: middle;background:url("../img/pro/product-sprits.png") no-repeat;width:90px;height:90px;}
.product-title .icon-manage{background-position:-3px -5px;}
.product-title .icon-business{background-position:-103px -5px;}
.product-title .icon-equipment{background-position:-202px -5px;}
.product-title .icon-General{background-position:-297px -5px;}
.product-title .icon-Distribution{background-position:-401px -5px;}
.product-title .icon-Monitoring{background-position:-510px -5px;}
.product-title .title{line-height: 66px;color:#c19e8b;font-size:26px;}
.product-title .icon-view{padding-top:20px;}
.product-slide-box{background-color:#fff;padding-bottom:40px;}
.product-shadow{background:url("../img/pro/product-bottomshadow_03.png") repeat-x;height:42px;position:absolute;left:0px;bottom:-13px;width:100%;z-index:222;}
.product-carsous{background:#fff;position:relative;height:380px;margin:30px auto 0px auto;}
.product-carsous ul{}
.product-carsous li{width:170px;float:left;display:inline;zoom:1;}
.product-carsous li .linkimg{width:170px;height:200px;display:block;}
.product-carsous li .linkimg img{width:170px;height:200px;}
.product-carsous li p{line-height: 20px;margin:15px 0 0 0;height:40px;overflow:hidden;}
.product-overlay{width:740px;padding-top:60px;margin:0 auto;}
.product-info{padding:20px 0 20px 0;border-bottom:1px solid #fff;}
.product-info p{line-height: 30px;text-indent:2em;font-size:12px;color:#666;}


/*slider自定义图片切换*/
.carsous .left { width:65px; height:65px; float:left; cursor:pointer;background:url("../img/pro/product-sprits.png") no-repeat -16px -113px; margin-top:140px;position:absolute;left:40px;top:0px;}
.carsous .right{ width:65px; height:65px; float:left; cursor:pointer;background:url("../img/pro/product-sprits.png") no-repeat -108px -113px;  margin-top:140px;position:absolute;right:40px;top:0px;}
.carsous .center { width:760px; height:367px;  overflow:hidden; position:relative; margin:0px auto;}
.carsous .center ul { width:auto;  top:60px; left:0px; position:absolute; overflow:hidden;}
.carsous .center li { float:left;padding:0px 10px; position:relative;}
.carsous .center li img { width:170px; height:200px;}
.carsous .scroll { width:760px; height:6px; left:0px; bottom:8px; cursor:pointer; position:absolute; background:#e6e2e0; border-radius:4px;-moz-border-radius:4px;-webkit-border-radius: 4px;}
.carsous .scroll span { width:48px; height:6px; left:0px; bottom:0px; cursor:pointer; position:absolute; background:#d3cdcb;border-radius:4px;-moz-border-radius:4px;-webkit-border-radius: 4px;height:6px; }



